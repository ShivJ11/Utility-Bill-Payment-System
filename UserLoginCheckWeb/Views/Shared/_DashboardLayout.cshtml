    <script>
        $(document).ready(function () {
            // Show loading div
            $('.loadingDiv-parent').show();

            // Hide loading div and show main container once page has finished loading
            $(window).on('load', function () {
                $('.loadingDiv-parent').fadeOut('slow', function () {
                    $('.main-container').fadeIn('slow');
                });
            });
        });

    </script>

